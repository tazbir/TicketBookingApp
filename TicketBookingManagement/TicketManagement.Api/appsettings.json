{
  "ConnectionStrings":{
    "TicketManagementConnectionString": "Server=localhost,1433;Database=TicketManagementDb111;integrated security=false;TrustServerCertificate=True;MultipleActiveResultSets=true;User Id=sa;Password=<YourStrong@Passw0rd>",
    "TicketManagementIdentityConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=TicketIdentityDb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EmailSettings": {
    "FromAddress": "",
    "ApiKey": "",
    "FromName": "Gill"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
